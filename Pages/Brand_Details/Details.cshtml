@page
@model Electronics_Items_Management.Pages.Brand_Details.DetailsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details Page</h1>

<div>
    <h4>Brand Detail</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            Brand Name
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Brand_Detail.Brand_Name)
        </dd>
        <dt class="col-sm-2">
           Brand Location
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Brand_Detail.Brand_Branch_Location)
        </dd>
        <dt class="col-sm-2">
           Email
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Brand_Detail.Brand_Email)
        </dd>
    </dl>
</div>
<div>
    <a asp-page="./Edit" asp-route-id="@Model.Brand_Detail.Id">Edit</a> |
    <a asp-page="./Index">GO TO HOME PAGE</a>
</div>
